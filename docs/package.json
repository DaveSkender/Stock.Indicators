    "@playwright/test": "^1.44.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "angular-eslint": "20.2.0",
    "eslint": "^9.33.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.1.1",
    "markdownlint": "^0.36.1",
    "markdownlint-cli": "^0.43.0",
    "ng-mocks": "^14.12.0",
    "typescript": "~5.6.0",
    "typescript-eslint": "8.40.0"
  }
