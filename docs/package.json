{
  "name": "stock-indicators-docs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "build:complete": "ng build && node generate-sitemap.js",
    "e2e": "npm run build && playwright test",
    "serve:static": "http-server dist/static -p 4000 --silent",
    "lint:md": "markdownlint 'public/**/*.md' '_indicators/**/*.md' '*.md' --ignore node_modules --ignore dist --ignore .angular",
    "lint:md:fix": "markdownlint --fix 'public/**/*.md' '_indicators/**/*.md' '*.md' --ignore node_modules --ignore dist --ignore .angular"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.3",
    "@angular/common": "^20.2.3",
    "@angular/compiler": "^20.2.3",
    "@angular/core": "^20.2.3",
    "@angular/forms": "^20.2.3",
    "@angular/platform-browser": "^20.2.3",
    "@angular/platform-browser-dynamic": "^20.2.3",
    "@angular/router": "^20.2.3",
    "marked": "^14.1.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.2.1",
    "@angular/cli": "^20.2.1",
    "@angular/compiler-cli": "^20.2.3",
    "@types/jest": "^29.5.12",
    "@types/marked": "^6.0.0",
    "http-server": "^14.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-util": "^29.7.0",
    "jest-preset-angular": "^14.1.0",
    "ts-jest": "^29.2.5",
    "@playwright/test": "^1.47.0",
    "markdownlint": "^0.36.1",
    "markdownlint-cli": "^0.43.0",
    "typescript": "~5.9.2"
  }
}
