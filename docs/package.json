{
  "name": "stock-indicators-docs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:complete": "ng build && node generate-sitemap.js",
    "e2e": "npm run build && playwright test",
    "e2e:ui": "playwright test --ui",
    "serve:static": "http-server dist/static -p 4000 --silent",
    "lint:md": "markdownlint 'public/**/*.md' '_indicators/**/*.md' '*.md' --ignore node_modules --ignore dist --ignore .angular",
    "lint:md:fix": "markdownlint --fix 'public/**/*.md' '_indicators/**/*.md' '*.md' --ignore node_modules --ignore dist --ignore .angular",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.2",
    "@angular/common": "^20.3.2",
    "@angular/compiler": "^20.3.2",
    "@angular/core": "^20.3.2",
    "@angular/forms": "^20.3.2",
    "@angular/platform-browser": "^20.3.2",
    "@angular/platform-browser-dynamic": "^20.3.2",
    "@angular/router": "^20.3.2",
    "marked": "^16.3.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.2",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "^20.3.2",
    "@playwright/test": "1.55.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@types/jest": "29.5.14",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.11.19",
    "angular-eslint": "20.2.0",
    "eslint": "^9.33.0",
    "http-server": "^14.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-preset-angular": "^14.1.1",
    "jest-util": "29.7.0",
    "markdownlint": "^0.36.1",
    "markdownlint-cli": "^0.43.0",
    "ng-mocks": "^14.12.0",
    "playwright-ng-schematics": "^2.1.0",
    "ts-jest": "29.2.5",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.40.0"
  }
}