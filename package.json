{
  "name": "stock-indicators",
  "description": "Stock Indicators for .NET",
  "author": "Dave Skender",
  "license": "Apache-2.0",
  "private": false,
  "packageManager": "npm@11.6.1",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "clean": "rimraf --glob \"**/bin\" \"**/obj\" \"**/TestResults\" \"**/BenchmarkDotNet.Artifacts\" \"node_modules\" \"**/node_modules\"",
    "lint:md": "markdownlint-cli2 \"**/*.md\"",
    "lint:md:fix": "markdownlint-cli2 --fix \"**/*.md\"",
    "mdfix": "markdownlint-cli2 --fix \"**/*.md\"",
    "test:coverage": "dotnet test \"Stock.Indicators.sln\" --collect:\"Code Coverage\" --no-restore --nologo"
  },
  "devDependencies": {
    "markdownlint-cli2": "^0.18.1",
    "rimraf": "^6.0.1"
  }
}
