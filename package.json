{
  "$schema":"https://www.schemastore.org/package",
  "name": "stock-indicators",
  "description": "Stock Indicators for .NET",
  "homepage": "https://stockindicators.dev",
  "fundingUrl":"https://github.com/sponsors/facioquo",
  "author": {
    "name": "FacioQuo",
    "url": "https://github.com/facioquo"
  },
  "license": "Apache-2.0",
  "private": false,
  "packageManager": "npm@11.6.2",
  "engines": {
    "node": ">=22.21.0",
    "npm": ">=11.6.2"
  },
  "scripts": {
    "clean": "npx rimraf -g \"**/bin\" \"**/obj\" \"**/TestResults\" \"**/vendor\" \"**/_site\" \"**/BenchmarkDotNet.Artifacts\" \".codacy\" \"node_modules\" \"**/node_modules\"",
    "lint:md": "markdownlint-cli2 \"**/*.md\"",
    "lint:md:fix": "markdownlint-cli2 --fix \"**/*.md\"",
    "fixall": "roslynator fix --properties TargetFramework=net9.0 && dotnet format --severity info --no-restore && markdownlint-cli2 --fix \"**/*.md\"",
    "test:coverage": "dotnet test \"Stock.Indicators.sln\" --collect:\"Code Coverage\" --no-restore --nologo"
  },
  "devDependencies": {
    "@codacy/codacy-mcp": "^0.6.18",
    "markdownlint-cli2": "^0.18.1"
  }
}
