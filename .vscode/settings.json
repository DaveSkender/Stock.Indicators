{
  "dotnet.defaultSolution": "Stock.Indicators.sln",
  "files.associations": {
    "*.md": "markdown"
  },
  "githubPullRequests.ignoredPullRequestBranches": [
    "v3"
  ],
  "search.exclude": {
    "**/_site/**": true,
    "**/vendor/**": true,
    "**/node_modules/**": true,
    "**/_testdata/**/*.csv": true,
    "**/_testdata/**/*.json": true,
    "**/.vs/**": true,
    "**/bin/**": true,
    "**/obj/**": true,
    "**/TestResults/**": true,
    "**/*.csproj.user": true,
    "**/*.log": true,
    "**/*.tmp": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
  }
}
